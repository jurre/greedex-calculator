{
  "name": "next-websocket-sever",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.server.json && next build",
    "start": "cross-env NODE_ENV=production node out/server.js",
    "lint": "biome check",
    "format": "biome format --write",
    "drizzle:pull": "drizzle-kit pull",
    "drizzle:push": "drizzle-kit push",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/better-auth/index.ts --output ./src/lib/drizzle/schemas/auth-schema.ts --yes"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.8",
    "@types/pg": "^8.15.6",
    "better-auth": "^1.3.34",
    "cross-env": "^10.1.0",
    "drizzle-orm": "^0.44.7",
    "next": "16.0.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
}
