{
  "name": "next-websocket-sever",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.server.json && next build",
    "start": "cross-env NODE_ENV=production node out/server.js",
    "start:clickdummy": "bunx http-server docs/greendex-clickdummy/build -p 5000",
    "lint": "biome check",
    "format": "biome check --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/better-auth/index.ts --output ./src/lib/drizzle/schemas/auth-schema.ts --yes"
  },
